{
  "extends": "justinlocsei/configurations/react",
  "env": {
    "commonjs": true
  },
  "globals": {
    "HIMATION_CONFIG": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "object-shorthand": 0,
    "react/no-danger": 0,
    "strict": [2, "never"]
  }
}
